@model IEnumerable<Category>
<h3> categories list</h3>
<table>
    <tr>
        <td>Name </td>
        <td>Descraption </td>
        <td>Data created </td>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td> @item.Name </td>
            <td>@item.Description </td>
            <td>@item.CreatedDate </td>
        </tr>
    }
</table>